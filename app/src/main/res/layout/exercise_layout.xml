<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="@color/background">

	<EditText
			android:id="@+id/time_input3"
			android:layout_width="67dp"
			android:layout_height="27dp"
			android:layout_marginStart="8dp"
			android:layout_marginTop="32dp"
			android:background="@color/colorAccent"
			android:ems="10"
			android:hint="seconds"
			android:inputType="number"
			android:textColor="@color/textColor"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintHorizontal_bias="0.0"
			app:layout_constraintStart_toEndOf="@+id/time_input2"
			app:layout_constraintTop_toBottomOf="@+id/textView2" />

	<TextView
			android:id="@+id/diet_textView"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="TextView"
			android:textColor="@color/textColor"
			android:textSize="20sp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintVertical_bias="0.022" />

	<TextView
			android:id="@+id/trainingdescription_textView"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="108dp"
			android:text="TextView"
			android:textColor="@color/textColor"
			android:textSize="20sp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/diet_textView"
			app:layout_constraintVertical_bias="0.0" />

	<TextView
			android:id="@+id/textView2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="60dp"
			android:text="Workout"
			android:textColor="@color/textColor"
			android:textSize="20sp"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/trainingdescription_textView" />

	<TextView
			android:id="@+id/textView4"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="28dp"
			android:layout_marginTop="32dp"
			android:text="Time:"
			android:textColor="@color/textColor"
			android:textSize="20sp"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/textView2" />

	<EditText
			android:id="@+id/time_input"
			android:layout_width="67dp"
			android:layout_height="27dp"
			android:layout_marginStart="12dp"
			android:layout_marginTop="32dp"
			android:background="@color/colorAccent"
			android:ems="10"
			android:hint="hours"
			android:inputType="number"
			android:textColor="@color/textColor"
			app:layout_constraintStart_toEndOf="@+id/textView4"
			app:layout_constraintTop_toBottomOf="@+id/textView2" />

	<EditText
			android:id="@+id/time_input2"
			android:layout_width="67dp"
			android:layout_height="27dp"
			android:layout_marginStart="8dp"
			android:layout_marginTop="32dp"
			android:background="@color/colorAccent"
			android:ems="10"
			android:hint="minutes"
			android:inputType="number"
			android:textColor="@color/textColor"
			app:layout_constraintStart_toEndOf="@+id/time_input"
			app:layout_constraintTop_toBottomOf="@+id/textView2" />

	<Button
			android:id="@+id/workout_button"
			android:layout_width="190dp"
			android:layout_height="53dp"
			android:layout_marginTop="28dp"
			android:background="@color/colorAccent"
			android:text="Done"
			android:textColor="@color/textColor"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintHorizontal_bias="0.498"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/time_input2" />


</androidx.constraintlayout.widget.ConstraintLayout>