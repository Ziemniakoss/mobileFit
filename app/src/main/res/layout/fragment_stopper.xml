<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
												   xmlns:app="http://schemas.android.com/apk/res-auto"
												   xmlns:tools="http://schemas.android.com/tools"
												   android:layout_width="match_parent"
												   android:layout_height="match_parent"
												   android:background="@color/background">

	<Button
			android:id="@+id/stopperstart_button"
			android:layout_width="200dp"
			android:layout_height="wrap_content"
			android:layout_marginStart="2dp"
			android:background="@color/colorAccent"
			android:text="Button"
			android:textColor="@color/textColor"
			app:layout_constraintEnd_toStartOf="@+id/stopperreset_button"
			app:layout_constraintHorizontal_bias="0.181"
			app:layout_constraintStart_toStartOf="parent"
			tools:layout_editor_absoluteY="545dp"/>

	<Button
			android:id="@+id/stopperreset_button"
			android:layout_width="200dp"
			android:layout_height="wrap_content"
			android:layout_marginEnd="3dp"
			android:background="@color/colorAccent"
			android:text="Button"
			android:textColor="@color/textColor"
			app:layout_constraintEnd_toEndOf="parent"
			tools:layout_editor_absoluteY="545dp"/>
	<TextView
			android:id="@+id/tv_hours5"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_marginTop="32dp"
			android:text="HH"
			android:textColor="@color/textColor" android:textSize="36sp"
			app:layout_constraintEnd_toStartOf="@+id/tv_hour_dotdot2"
			app:layout_constraintHorizontal_bias="0.5" app:layout_constraintStart_toEndOf="@+id/tv_hour_dotdot"
			app:layout_constraintTop_toTopOf="parent"/>
	<TextView
			android:id="@+id/tv_hours7"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_marginTop="32dp"
			android:text="HH"
			android:textColor="@color/textColor" android:textSize="36sp"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintHorizontal_bias="0.5" app:layout_constraintStart_toEndOf="@+id/tv_hour_dotdot2"
			app:layout_constraintTop_toTopOf="parent"/>
	<TextView
			android:id="@+id/tv_hours6"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_marginTop="32dp"
			android:text="HH"
			android:textColor="@color/textColor" android:textSize="36sp"
			app:layout_constraintEnd_toStartOf="@+id/tv_hour_dotdot" app:layout_constraintHorizontal_bias="0.5"
			app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"/>
	<TextView
			android:id="@+id/tv_hour_dotdot"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text=":" android:textColor="@color/cardview_light_background"
			app:layout_constraintEnd_toStartOf="@+id/tv_hours5"
			app:layout_constraintHorizontal_bias="0.5" app:layout_constraintStart_toEndOf="@+id/tv_hours6"
			tools:layout_editor_absoluteY="49dp"/>
	<TextView
			android:id="@+id/tv_hour_dotdot2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text=":" android:textColor="@color/cardview_light_background"
			app:layout_constraintEnd_toStartOf="@+id/tv_hours7"
			app:layout_constraintHorizontal_bias="0.5" app:layout_constraintStart_toEndOf="@+id/tv_hours5"
			tools:layout_editor_absoluteY="49dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>